{
  "name": "pic",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "npm run api",
    "api": "cross-env NODE_ENV=test jest --config test/api/jest.config.js"
  },
  "dependencies": {
    "cross-env": "^5.2.0",
    "dotenv": "^6.1.0",
    "element-ui": "^2.4.5",
    "koa": "^2.5.3",
    "koa-body": "^4.0.4",
    "koa-router": "^7.4.0",
    "mysql2": "^1.6.1",
    "register-service-worker": "^1.0.0",
    "sequelize": "^4.41.0",
    "sequelize-cli": "^5.2.0",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.4",
    "@vue/cli-plugin-eslint": "^3.0.4",
    "@vue/cli-plugin-pwa": "^3.0.4",
    "@vue/cli-service": "^3.0.4",
    "@vue/eslint-config-standard": "^3.0.5",
    "babel-plugin-component": "^1.1.1",
    "eslint": "^5.7.0",
    "eslint-plugin-vue@next": "^4.7.1",
    "jest": "^23.6.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "supertest": "^3.3.0",
    "vue-cli-plugin-element": "^1.0.0",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "@vue/standard"
    ],
    "rules": {
      "prefer-promise-reject-errors": [
        0
      ],
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "vue/max-attributes-per-line": [
        2,
        {
          "singleline": 2
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
