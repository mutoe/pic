.el-pagination
  white-space nowrap
  padding 2px 5px
  color $color-text-primary
  font-weight 700
  clearfix()

  .el-input__inner
    text-align center
    appearance textfield

  .el-input__suffix
    right 0
    transform scale(0.8)

  .el-select
    .el-input
      width 100px
      margin 0 5px

      .el-input__inner
        padding-right 25px
        border-radius 3px
        height 28px

  button
    border none
    padding 0 6px
    background 0 0

    &:focus
      outline 0

    &:hover
      color $color-primary

    &.disabled
      color $color-text-placeholder
      background-color $color-white
      cursor not-allowed

  .btn-prev
    padding-right 12px

  .btn-next
    padding-left 12px

  .el-pager
    li
      &.disabled
        color $color-text-placeholder
        cursor not-allowed

  &.is-background
    .el-pager
      li
        &:not(.disabled)
          &:hover
            color $color-primary

          &.active
            background-color $color-primary
            color $color-white

.el-pagination button
.el-pagination span:not([class*="suffix"])
  display inline-block
  font-size 13px
  min-width 35.5px
  height 28px
  line-height 28px
  vertical-align top
  box-sizing border-box

.el-pager li
.el-pagination__editor
  box-sizing border-box
  text-align center

.el-pagination .btn-next
.el-pagination .btn-prev
  background center center no-repeat $color-white
  background-size 16px
  cursor pointer
  margin 0
  color $color-text-primary

.el-pagination .btn-next .el-icon
.el-pagination .btn-prev .el-icon
  display block
  font-size 12px
  font-weight 700

.el-pager li
.el-pager li.btn-quicknext:hover
.el-pager li.btn-quickprev:hover
  cursor pointer

.el-pagination--small .btn-next
.el-pagination--small .btn-prev
.el-pagination--small .el-pager li
.el-pagination--small .el-pager li:last-child
  border-color transparent
  font-size 12px
  line-height 22px
  height 22px
  min-width 22px

.el-pagination__sizes
  margin 0 10px 0 0
  font-weight 400
  color $color-text-regular

  .el-input
    .el-input__inner
      font-size 13px
      padding-left 8px

      &:hover
        border-color $color-primary

.el-pagination__total
  margin-right 10px
  font-weight 400
  color $color-text-regular

.el-pagination__jump
  margin-left 24px
  font-weight 400
  color $color-text-regular

  .el-input__inner
    padding 0 3px

.el-pagination__rightwrapper
  float right

.el-pagination__editor
  line-height 18px
  padding 0 2px
  height 28px
  margin 0 2px
  box-sizing border-box
  border-radius 3px

  &.el-input
    width 50px

    .el-input__inner
      height 28px

.el-pager
.el-pagination.is-background .btn-next
.el-pagination.is-background .btn-prev
  padding 0

.el-pagination__editor .el-input__inner::-webkit-inner-spin-button
.el-pagination__editor .el-input__inner::-webkit-outer-spin-button
  appearance none
  margin 0

.el-pagination.is-background .btn-next
.el-pagination.is-background .btn-prev
.el-pagination.is-background .el-pager li
  margin 0 5px
  background-color $color-info-lighter
  color $color-text-regular
  min-width 30px
  border-radius $border-radius-small

.el-pagination.is-background .btn-next.disabled
.el-pagination.is-background .btn-prev.disabled
.el-pagination.is-background .el-pager li.disabled
  color $color-text-placeholder

.el-pagination.is-background.el-pagination--small .btn-next
.el-pagination.is-background.el-pagination--small .btn-prev
.el-pagination.is-background.el-pagination--small .el-pager li
  margin 0 3px
  min-width 22px

.el-pager
.el-pager li
  vertical-align top
  margin 0
  display inline-block

.el-pager
  user-select none
  list-style none
  font-size 0

  .more::before
    line-height 30px

  li
    padding 0 4px
    background $color-white
    font-size 13px
    min-width 35.5px
    height 28px
    line-height 28px
    box-sizing border-box

    &.active
      color $color-primary
      cursor default

      +li
        border-left 0

    &:hover
      color $color-primary

.el-radio
.el-table th
  user-select none

.el-date-table
.el-radio
.el-table th
  user-select none

.el-menu--collapse .el-menu .el-submenu
.el-menu--popup
  min-width 200px

.el-dialog
.el-dialog__footer
  box-sizing border-box

.el-pager li.btn-quicknext
.el-pager li.btn-quickprev
  line-height 28px
  color $color-text-primary

.el-pager li.btn-quicknext.disabled
.el-pager li.btn-quickprev.disabled
  color $color-text-placeholder

.el-dialog
  position relative
  margin 0 auto 50px
  background $color-white
  border-radius $border-radius-small
  box-shadow $dialog-box-shadow
  box-sizing border-box
  width 50%

  &.is-fullscreen
    width 100%
    margin-top 0
    margin-bottom 0
    height 100%
    overflow auto

.el-dialog__wrapper
  position fixed
  top 0
  right 0
  bottom 0
  left 0
  overflow auto
  margin 0

.el-dialog__header
  padding 20px 20px 10px

.el-dialog__headerbtn
  position absolute
  top 20px
  right 20px
  padding 0
  background 0 0
  border none
  outline 0
  cursor pointer
  font-size 16px

  .el-dialog__close
    color $color-info

.el-dialog__headerbtn:focus .el-dialog__close
.el-dialog__headerbtn:hover .el-dialog__close
  color $color-primary

.el-dialog__title
  line-height 24px
  font-size 18px
  color $color-text-primary

.el-dialog__body
  padding 30px 20px
  color $color-text-regular
  line-height 24px
  font-size 14px

.el-dialog__footer
  padding 10px 20px 20px
  text-align right
  box-sizing border-box

.el-dialog--center
  text-align center

  .el-dialog__body
    text-align initial
    padding 25px 25px 30px

  .el-dialog__footer
    text-align inherit

.dialog-fade-enter-active
  animation dialog-fade-in 0.3s

.dialog-fade-leave-active
  animation dialog-fade-out 0.3s

@keyframes dialog-fade-in
  0%
    transform translate3d(0, -20px, 0)
    opacity 0

  100%
    transform translate3d(0, 0, 0)
    opacity 1

@keyframes dialog-fade-out
  0%
    transform translate3d(0, 0, 0)
    opacity 1

  100%
    transform translate3d(0, -20px, 0)
    opacity 0

.el-autocomplete
  position relative
  display inline-block

.el-autocomplete-suggestion
  margin 5px 0
  box-shadow $box-shadow-light
  border-radius $border-radius-base

  li
    padding 0 20px
    margin 0
    line-height 34px
    cursor pointer
    color $color-text-regular
    font-size 14px
    list-style none
    white-space nowrap
    overflow hidden
    text-overflow ellipsis

    &.divider
      margin-top 6px
      border-top 1px solid $color-black

      &:last-child
        margin-bottom -6px

  &.is-loading
    li
      text-align center
      height 100px
      line-height 100px
      font-size 20px
      color $color-13

      &:hover
        background-color $color-white

    li::after
      display inline-block
      content ""
      height 100%
      vertical-align middle

    .el-icon-loading
      vertical-align middle

.el-dropdown-menu
.el-menu--collapse .el-submenu .el-menu
  z-index 10
  box-shadow $box-shadow-light

.el-autocomplete-suggestion__wrap
  max-height 280px
  padding 10px 0
  box-sizing border-box
  overflow auto
  background-color $color-white
  border 1px solid $border-color-light
  border-radius $border-radius-base

.el-autocomplete-suggestion__list
  margin 0
  padding 0

.el-autocomplete-suggestion li.highlighted
.el-autocomplete-suggestion li:hover
  background-color $color-background-base

.el-dropdown
  display inline-block
  position relative
  color $color-text-regular
  font-size 14px

  .el-button-group
    display block

    .el-button
      float none

  .el-dropdown__caret-button
    padding-left 5px
    padding-right 5px
    position relative
    border-left none

    .el-dropdown__icon
      padding-left 0

  .el-dropdown__caret-button::before
    content ""
    position absolute
    display block
    width 1px
    top 5px
    bottom 5px
    left 0
    background rgba(#fff, 0.5)

    &:hover
      top 0
      bottom 0

.el-dropdown__icon
  font-size 12px
  margin 0 3px

.el-dropdown .el-dropdown-selfdefine:focus:active
.el-dropdown .el-dropdown-selfdefine:focus:not(.focusing)
  outline-width 0

.el-dropdown-menu
  position absolute
  top 0
  left 0
  padding 10px 0
  margin 5px 0
  background-color $color-white
  border 1px solid $border-color-lighter
  border-radius $border-radius-base
  box-shadow $box-shadow-light

.el-dropdown-menu__item
  list-style none
  line-height 36px
  padding 0 20px
  margin 0
  font-size 14px
  color $color-text-regular
  cursor pointer
  outline 0

  &.is-disabled
    cursor default
    color $color-text-disabled
    pointer-events none

.el-dropdown-menu__item:focus
.el-dropdown-menu__item:not(.is-disabled):hover
  background-color $color-primary-light-9
  color $color-primary-light-2

.el-dropdown-menu__item--divided:before
.el-menu
.el-menu--horizontal > .el-menu-item:not(.is-disabled):focus
.el-menu--horizontal > .el-menu-item:not(.is-disabled):hover
.el-menu--horizontal > .el-submenu .el-submenu__title:hover
  background-color $color-white

.el-dropdown-menu__item--divided
  position relative
  margin-top 6px
  border-top 1px solid $border-color-lighter

  &:before
    content ""
    height 6px
    display block
    margin 0 -20px

.el-dropdown-menu--medium
  padding 6px 0

  .el-dropdown-menu__item
    line-height 30px
    padding 0 17px
    font-size 14px

.el-dropdown-menu--medium
  .el-dropdown-menu__item
    &.el-dropdown-menu__item--divided
      margin-top 6px

      &:before
        height 6px
        margin 0 -17px

.el-dropdown-menu--small
  padding 6px 0

  .el-dropdown-menu__item
    line-height 27px
    padding 0 15px
    font-size 13px

.el-dropdown-menu--small
  .el-dropdown-menu__item
    &.el-dropdown-menu__item--divided
      margin-top 4px

      &:before
        height 4px
        margin 0 -15px

.el-dropdown-menu--mini
  padding 3px 0

  .el-dropdown-menu__item
    line-height 24px
    padding 0 10px
    font-size 12px

.el-dropdown-menu--mini
  .el-dropdown-menu__item
    &.el-dropdown-menu__item--divided
      margin-top 3px

      &:before
        height 3px
        margin 0 -10px

.el-menu
  border-right solid 1px #e6e6e6
  list-style none
  position relative
  margin 0
  padding-left 0
  clearfix()

.el-menu--horizontal
  border-right none
  border-bottom solid 1px #e6e6e6

  >.el-menu-item
    float left
    height 60px
    line-height 60px
    margin 0
    border-bottom 2px solid transparent
    color $color-info

    &.is-active
      border-bottom 2px solid $color-primary
      color $color-text-primary

  >.el-submenu
    float left

    &.is-active
      .el-submenu__title
        border-bottom 2px solid $color-primary
        color $color-text-primary

    .el-submenu__title
      height 60px
      line-height 60px
      border-bottom 2px solid transparent
      color $color-info

    .el-submenu__icon-arrow
      position static
      vertical-align middle
      margin-left 8px
      margin-top -3px

.el-menu--horizontal > .el-menu-item a
.el-menu--horizontal > .el-menu-item a:hover
  color inherit

.el-menu--horizontal > .el-submenu:focus
.el-menu--horizontal > .el-submenu:hover
  outline 0

.el-menu--horizontal > .el-submenu:focus .el-submenu__title
.el-menu--horizontal > .el-submenu:hover .el-submenu__title
  color $color-text-primary

.el-menu--horizontal .el-menu .el-menu-item
.el-menu--horizontal .el-menu .el-submenu__title
  background-color $color-white
  float none
  height 36px
  line-height 36px
  padding 0 10px
  color $color-info

.el-menu--horizontal .el-menu .el-menu-item.is-active
.el-menu--horizontal .el-menu .el-submenu__title.is-active
  color $color-text-primary

.el-menu--horizontal .el-menu-item:not(.is-disabled):focus
.el-menu--horizontal .el-menu-item:not(.is-disabled):hover
  outline 0
  color $color-text-primary

.el-menu--collapse
  width 64px

  >.el-menu-item
    &.is-active
      i
        color inherit

  .el-submenu
    position relative

    .el-menu
      position absolute
      margin-left 5px
      top 0
      left 100%
      border 1px solid $border-color-light
      border-radius $border-radius-small
      box-shadow $box-shadow-light

.el-menu--collapse > .el-menu-item [class^="el-icon-"]
.el-menu--collapse > .el-submenu > .el-submenu__title [class^="el-icon-"]
  margin 0
  vertical-align middle
  width 24px
  text-align center

.el-menu--collapse > .el-menu-item .el-submenu__icon-arrow
.el-menu--collapse > .el-submenu > .el-submenu__title .el-submenu__icon-arrow
  display none

.el-menu--collapse > .el-menu-item span
.el-menu--collapse > .el-submenu > .el-submenu__title span
  height 0
  width 0
  overflow hidden
  visibility hidden
  display inline-block

.el-menu-item
.el-submenu__title
  height 56px
  line-height 56px
  position relative
  box-sizing border-box
  white-space nowrap
  list-style none

.el-menu--collapse


    .el-submenu

      &.is-opened

          >
            .el-submenu__title


                .el-submenu__icon-arrow
                  transform none

.el-menu--popup
  z-index 100
  border none
  padding 5px 0
  border-radius $border-radius-small
  box-shadow $box-shadow-light

.el-menu--popup-bottom-start
  margin-top 5px

.el-menu--popup-right-start
  margin-left 5px

.el-menu-item
  font-size 14px
  color $color-text-primary
  padding 0 20px
  cursor pointer
  transition border-color 0.3s, background-color 0.3s, color 0.3s
  box-sizing border-box

  *
    vertical-align middle

  i
    color $color-info

  &.is-disabled
    opacity 0.25
    cursor not-allowed
    background 0 0 !important

  [class^="el-icon-"]
    margin-right 5px
    width 24px
    text-align center
    font-size 18px
    vertical-align middle

  &.is-active
    color $color-primary

    i
      color inherit

.el-menu-item:focus
.el-menu-item:hover
  outline 0
  background-color $color-primary-light-9

.el-submenu
  list-style none
  margin 0
  padding-left 0

  .el-menu
    border none

  .el-menu-item
    height 50px
